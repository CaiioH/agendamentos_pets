{
  "ReverseProxy": {
    "Routes": {
      "petapi": {
        "Match": {
          "Path": "/api/pet/{**catch-all}"
        },
        "ClusterId": "petapi"
      },
      "petapi-racas": {
        "Match": {
          "Path": "/api/raca/{**catch-all}"
        },
        "ClusterId": "petapi"
      },
      "agendaapi": {
        "Match": {
          "Path": "/api/agendamento/{**catch-all}"
        },
        "ClusterId": "agendaapi"
      }
    },
    "Clusters": {
      "petapi": {
        "Destinations": {
          "petapi1": {
            "Address": "http://petapi:8080"
          }
        }
      },
      "agendaapi": {
        "Destinations": {
          "agendaapi1": {
            "Address": "http://agendaapi:8080"
          }
        }
      }
    }
  }
}
